@if (this.TempData["ErrorMessage"] != null)
{
    <div id="notification" class="alert alert-danger text-center alert-dismissible fade show custom-margin-identity mt-3" role="alert">
        @this.TempData["ErrorMessage"]
        <button type="button" class="accept-policy close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">OK</span>
        </button>
    </div>
}
